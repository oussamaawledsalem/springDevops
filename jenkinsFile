pipeline {
    agent any

    tools {
        maven "M2_HOME"
    }

    environment {
        SONAR_TOKEN = 'ton_token_sonarqube'  // Remplace par ton token
    }

    stages {
        stage('GIT') {
            steps {
                git branch: 'main', url: 'https://github.com/oussamaawledsalem/springDevops.git'
            }
        }

        stage('COMPILE') {
            steps {
                sh 'mvn clean compile'
            }
        }


        stage('NEXUS DEPLOY') {
            steps {
                sh 'mvn deploy -DskipTests'
            }
        }
    }
}
